pipeline {          options {            timeout(time: 1, unit: 'HOURS')             }           agent any           stages {                     stage('build and push') {             steps {                      sh'sudo docker build -t aymenchab/[image name]:[version] .'                                      withDockerRegistry([url: "", credentialsId: "dockerhub-id"]) {                        sh"docker push aymenchab/[image name]:[version]"                }                }                }            stage ('make pod') {                steps {                     sh"sudo kubectl apply -f [deployment file name].yml --namespace=test"           }           }           stage ('make service') {           steps {                     sh"sudo kubectl apply -f [service file name].yml --namespace=test"           }           }          }            }